<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="Get-from-KVM" mapIdentifier="Credentials">
    <DisplayName>Get from KVM</DisplayName>
    <Properties/>

<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ExtractVariables async="false" continueOnError="false" enabled="true" name="Extract-Variables">
    <DisplayName>Extract Variables</DisplayName>
    <Properties/>
    <Source clearPayload="false">request</Source>
    <XMLPayload stopPayloadProcessing="false">
        <Namespaces/>
        <Variable name="credentials.username" type="string">
            <XPath>//request/username</XPath>
        </Variable>
        <Variable name="private.credentials.password" type="string">
            <XPath>//request/password</XPath>
        </Variable>
    </XMLPayload>
</ExtractVariables>